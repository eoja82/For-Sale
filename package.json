{
  "name": "personal-for-sale-website",
  "private": true,
  "description": "A template website to list personal items for sale.",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "npx gatsby build",
    "develop": "npx gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "gatsby": "^4.12.1",
    "gatsby-plugin-react-helmet": "^5.12.1",
    "gatsby-source-filesystem": "^4.12.1",
    "gatsby-transformer-json": "^4.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eoja82/For-Sale"
  },
  "bugs": {
    "url": "https://github.com/eoja82/For-Sale/issues"
  }
}
